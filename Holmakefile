OPTIONS = QUIT_ON_FAILURE
INCLUDES= $(CAKEMLDIR)/translator $(CAKEMLDIR)/repl
ifdef POLY
HOLHEAP = heap
PARENT_HOLHEAP = $(CAKEMLDIR)/translator/local-hol-heap
POLY_CLEANS = $(HOLHEAP) $(HOLHEAP).o
BARE_THYS = $(CAKEMLDIR)/translator/word_preludeTheory $(CAKEMLDIR)/repl/repl_computeLib $(CAKEMLDIR)/repl/repl_computeTheory
DEPS = $(patsubst %,%.uo,$(BARE_THYS))
$(HOLHEAP) : $(DEPS)
	$(protect $(HOLDIR)/bin/buildheap) -b $(PARENT_HOLHEAP) -o $(HOLHEAP) $(BARE_THYS)
endif
